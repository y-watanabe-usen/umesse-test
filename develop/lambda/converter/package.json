{
  "name": "umesse-converter",
  "description": "U MESSE AUDIO CONVERTER",
  "version": "0.0.1",
  "private": true,
  "main": "lambda.js",
  "dependencies": {},
  "devDependencies": {
    "aws-sdk": "^2.442.0",
    "jest": "^26.6.3",
    "umesse-lib": "file:../layer/nodejs/node_modules/umesse-lib"
  },
  "scripts": {
    "build": "npm install --no-save $(npm pack ../layer/nodejs/node_modules/umesse-lib/ | tail -1)",
    "build:prod": "npm install --production",
    "pretest": "sh ./__tests__/init.sh",
    "test": "PATH=$PATH:../layer/bin jest -i --coverage"
  },
  "jest": {
    "verbose": false,
    "testMatch": [
      "**/__tests__/*.test.js"
    ]
  }
}
